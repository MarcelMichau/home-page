{
  "name": "home-page",
  "version": "2.1.0",
  "description": "Personal Website for Marcel Michau",
  "main": "n/a",
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/cli": "^4.1.18",
    "@types/node": "^25.0.10",
    "tailwindcss": "^4.1.18",
    "wrangler": "^4.60.0"
  },
  "scripts": {
    "start": "pnpm exec wrangler pages dev src --port 8788",
    "build": "pnpm exec tailwindcss -i ./src/main.css -o ./src/dist/main.css --minify",
    "build:watch": "pnpm exec tailwindcss -i ./src/main.css -o ./src/dist/main.css --watch",
    "test": "pnpm exec playwright test"
  },
